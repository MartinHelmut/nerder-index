{
  "name": "nerder-index",
  "version": "0.2.8",
  "description": "Calculate a difficulty for a given functions.",
  "main": "dist/index.js",
  "esnext": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "build": "babel lib --out-dir dist",
    "test": "jest",
    "lint": "eslint lib/*.js",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "ci": "npm run lint && npm run format:check && npm run test",
    "commit": "git-cz",
    "release": "standard-version -a",
    "prepare": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/nerder-index.git"
  },
  "keywords": [
    "complexity",
    "nerder",
    "difficulty",
    "index"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.se> (https://martin-fieber.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/nerder-index/issues"
  },
  "homepage": "https://github.com/MartinHelmut/nerder-index#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/lib"
    ],
    "testURL": "http://localhost/"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "baseBranches": [
      "main"
    ],
    "supportPolicy": [
      "lts_latest"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": "",
    "packageRules": [
      {
        "updateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      }
    ]
  },
  "dependencies": {
    "escomplex": "^2.0.0-alpha"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/preset-env": "7.14.1",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.25.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "standard-version": "9.2.0"
  }
}
