{
  "name": "nerder-index",
  "version": "0.2.3",
  "description": "Calculate a difficulty for a given functions.",
  "main": "dist/index.js",
  "esnext": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "eslint lib/*.js && jest",
    "build": "babel lib --out-dir dist",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write \"lib/*.js\"",
    "commit": "git-cz",
    "release": "standard-version -a",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/nerder-index.git"
  },
  "keywords": [
    "complexity",
    "nerder",
    "difficulty"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/nerder-index/issues"
  },
  "homepage": "https://github.com/MartinHelmut/nerder-index#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/lib"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": ""
  },
  "dependencies": {
    "escomplex": "^2.0.0-alpha"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "22.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-preset-env": "1.6.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "jest": "22.4.2",
    "prettier": "1.11.1",
    "standard-version": "4.3.0"
  }
}
